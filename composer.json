{
    "name": "smacp/machine-translator",
    "description": "PHP Machine translator",
    "keywords": ["machine", "translation", "xliff", "xlf"],
    "homepage": "https://github.com/smacp/machine-translator",
    "license": "MIT",
    "authors": [
        {
            "name": "Stuart MacPherson"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-dom": "*",
        "ext-simplexml": "*",
        "guzzlehttp/guzzle": "^7.3",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "smacp\\MachineTranslator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "smacp\\MachineTranslator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
